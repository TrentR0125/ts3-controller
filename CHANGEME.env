API_ENV=development
# The environment the API is running in (e.g. develop, staging, production)

# ============= API CONFIGURATION =============
API_HOST=http://localhost:3000
# The host or base URL of your API (e.g. http://localhost or https://api.example.com)

API_PORT=3000
# The port number your API will run on (e.g. 3000)

API_KEY=
# A key used for authenticating requests to your API or external APIs

JWT_SECRET=
# A secret string used to sign and verify JWT tokens for authentication

#ENCRYPT_SECRET=

# ============= MYSQL CONFIGURATION =============
MYSQL_HOST=localhost
# Hostname or IP of the MySQL server (e.g. localhost)

MYSQL_PORT=3306
# Port number for MySQL (default: 3306)

MYSQL_USER=root
# Username for the MySQL connection

MYSQL_PASSWORD=password
# Password for the MySQL connection

MYSQL_DATABASE=
# Database name your app will use

# ============= DISCORD CONFIGURATION =============
DISCORD_CID=
# Discord Client ID from the Discord Developer Portal (for your application/bot)

DISCORD_SECRET=
# Discord Client Secret from the Developer Portal (keep this safe!)

DISCORD_CALLBACK_URL=
# The OAuth2 callback/redirect URL registered in your Discord application settings

# ============= TEAMSPEAK CONFIGURATION =============
TEAMSPEAK_HOST=localhost
# The hostname or IP address of the TeamSpeak server

TEAMSPEAK_SERVER_PORT=9987
# The port number TeamSpeak uses for client connections (default: 9987)

TEAMSPEAK_QUERY_PORT=10011
# The ServerQuery port for admin API access (default: 10011)

TEAMSPEAK_USERNAME=serveradmin
# The ServerQuery username for authentication

TEAMSPEAK_PASSWORD=
# The ServerQuery password for authentication

TS_NICKNAME=TS3 Controller
# The nickname this application will use when connecting to TeamSpeak

